{
    "extends": [
        "stylelint-config-wordpress/scss",
        "stylelint-config-recess-order"
    ],
    "plugins": [
        "stylelint-scss"
    ],
    "rules": {
        "max-line-length" : 9999,
        "selector-class-pattern": null,
        "at-rule-no-unknown": null,
        "scss/at-rule-no-unknown": true
    }
}